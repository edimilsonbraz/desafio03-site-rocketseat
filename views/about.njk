{% extends "layout.njk" %}

{% block head %}
    <title>Sobre - Rocketseat</title>
{% endblock %}

{% block content %}
    <div class="conteudo">
        <img src="{{ about.logo_url }}" alt=" Imagem de {{ about.name}}">
        <h1>{{ about.name }}</h1>
        <p>{{ about.description}}</p>
    
        <div id="lista">           
                <ul>
                    {% for lista in about.listas %}
                        <li> {{lista.name}}</li>
                    {% endfor %}                                
                </ul>            
        </div>  
    </div>    
{% endblock %}
